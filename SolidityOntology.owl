#################################################################
#    Classes
#################################################################

###  http://www.solidityontology.oeg.upm.es#Abstract
:Abstract rdf:type owl:Class ;
          rdfs:subClassOf :ContractImplementation ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasRestrictedFunction ;
                            owl:someValuesFrom :RestrictedFunctionSpecification
                          ] ;
          rdfs:comment "A contract which may have unimplemented functions" ;
          rdfs:label "Abstract" .


###  http://www.solidityontology.oeg.upm.es#Address
:Address rdf:type owl:Class ;
         rdfs:subClassOf :ElementaryType ;
         rdfs:label "Address" .


###  http://www.solidityontology.oeg.upm.es#ArrayDimension
:ArrayDimension rdf:type owl:Class ;
                rdfs:subClassOf [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasIndex ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onDataRange xsd:integer
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasLenght ;
                                  owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onDataRange xsd:integer
                                ] ;
                rdfs:comment "The number of indices needed in order to specify an individual element of the array and its value" ;
                rdfs:label "Array Dimension" .


###  http://www.solidityontology.oeg.upm.es#ArrayType
:ArrayType rdf:type owl:Class ;
           rdfs:subClassOf :ParameterTypeWithDataLocation ,
                           :SimpleType ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasArrayDimension ;
                             owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass :ArrayDimension
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasType ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass :SimpleType
                           ] ;
           rdfs:label "Array Type" .


###  http://www.solidityontology.oeg.upm.es#ArrayValue
:ArrayValue rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :hasSimpleArrayValue ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :SimpleValue
                            ] ;
            rdfs:label "Array Value" .


###  http://www.solidityontology.oeg.upm.es#ArrayValueSpecification
:ArrayValueSpecification rdf:type owl:Class ;
                         rdfs:subClassOf :ValueSpecification ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :hasArrayValue ;
                                           owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onClass :ArrayValue
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :hasIndex ;
                                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onDataRange xsd:integer
                                         ] ;
                         rdfs:label "Array Value Specification" .


###  http://www.solidityontology.oeg.upm.es#ArrayVariableSpecification
:ArrayVariableSpecification rdf:type owl:Class ;
                            rdfs:subClassOf :ConstantAttributeSpecification ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty :hasArrayType ;
                                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                              owl:onClass :ArrayType
                                            ] ;
                            rdfs:label "Array Variable Specification" .


###  http://www.solidityontology.oeg.upm.es#Bool
:Bool rdf:type owl:Class ;
      rdfs:subClassOf :ElementaryType ;
      rdfs:label "Bool" .


###  http://www.solidityontology.oeg.upm.es#BoolValue
:BoolValue rdf:type owl:Class ;
           rdfs:subClassOf :SimpleValue ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasValue ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onDataRange xsd:boolean
                           ] ;
           rdfs:label "Bool Value" .


###  http://www.solidityontology.oeg.upm.es#Bytes
:Bytes rdf:type owl:Class ;
       rdfs:subClassOf :ParameterTypeWithDataLocation ,
                       :SingleMemoryType ;
       rdfs:label "Bytes" .


###  http://www.solidityontology.oeg.upm.es#BytesValue
:BytesValue rdf:type owl:Class ;
            rdfs:subClassOf :SimpleValue ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasValue ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:hexBinary
                            ] ;
            rdfs:label "Bytes Value" .


###  http://www.solidityontology.oeg.upm.es#CompleteFunctionSpecification
:CompleteFunctionSpecification rdf:type owl:Class ;
                               rdfs:subClassOf :RestrictedFunctionSpecification ,
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty :hasCode ;
                                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                 owl:onDataRange xsd:string
                                               ] ;
                               rdfs:label "Complete Function Specification" .


###  http://www.solidityontology.oeg.upm.es#ConstantAttributeSpecification
:ConstantAttributeSpecification rdf:type owl:Class ;
                                rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                  owl:onProperty :hasName ;
                                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                  owl:onDataRange xsd:string
                                                ] ;
                                rdfs:comment "This type of attributes cant have a value nor a visibility." ;
                                rdfs:label "Constant Attribute Specification" .


###  http://www.solidityontology.oeg.upm.es#ConstructorSpecification
:ConstructorSpecification rdf:type owl:Class ;
                          rdfs:subClassOf [ rdf:type owl:Restriction ;
                                            owl:onProperty :hasConstructorArguments ;
                                            owl:someValuesFrom :ParameterVariableSpecification
                                          ] ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty :hasCode ;
                                            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                            owl:onDataRange xsd:string
                                          ] ;
                          rdfs:label "Constructor Specification" .


###  http://www.solidityontology.oeg.upm.es#ContractImplementation
:ContractImplementation rdf:type owl:Class ;
                        rdfs:subClassOf :Implementation ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty :hasConstantAttribute ;
                                          owl:someValuesFrom :ConstantAttributeSpecification
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty :hasEvent ;
                                          owl:someValuesFrom :EventSpecification
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty :hasFunction ;
                                          owl:someValuesFrom :CompleteFunctionSpecification
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty :hasModifier ;
                                          owl:someValuesFrom :ModifierSpecification
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty :hasNonConstantAttribute ;
                                          owl:someValuesFrom :NonconstantAttributeSpecification
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty :hasConstructor ;
                                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onClass :ConstructorSpecification
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty :hasFallbackFunction ;
                                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onClass :FallbackSpecification
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty :hasInterface ;
                                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onClass :Interface
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty :hasReceiveFunction ;
                                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onClass :ReceiveSpecification
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty :isAbstract ;
                                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onClass :Abstract
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty :hasInheritance ;
                                          owl:someValuesFrom xsd:string
                                        ] ;
                        rdfs:comment "A computer program that includes variables, complete functions, events and all the code that perform the logic to achieve the contract objective" ;
                        rdfs:label "Contract Implementation" .


###  http://www.solidityontology.oeg.upm.es#ContractType
:ContractType rdf:type owl:Class ;
              rdfs:subClassOf :MapType ,
                              :ModifierOverrideType ,
                              :ParameterType ,
                              :SimpleType ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasContractType ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :ContractImplementation
                              ] ;
              rdfs:label "Contract Type" .


###  http://www.solidityontology.oeg.upm.es#ContractValue
:ContractValue rdf:type owl:Class ;
               rdfs:subClassOf :SimpleValue ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasContractValue ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass :ContractImplementation
                               ] ;
               rdfs:label "Contract Value" .


###  http://www.solidityontology.oeg.upm.es#DataLocation
:DataLocation rdf:type owl:Class ;
              rdfs:label "Data location" .


###  http://www.solidityontology.oeg.upm.es#DualMemoryType
:DualMemoryType rdf:type owl:Class ;
                rdfs:subClassOf :MemoryType ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasMemoryM ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onDataRange xsd:short
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasMemoryN ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onDataRange xsd:short
                                ] ;
                rdfs:label "Dual Memory Type" .


###  http://www.solidityontology.oeg.upm.es#ElementaryType
:ElementaryType rdf:type owl:Class ;
                rdfs:subClassOf :MapType ,
                                :ParameterType ,
                                :SimpleType ;
                rdfs:label "Elementary Type" .


###  http://www.solidityontology.oeg.upm.es#EnumType
:EnumType rdf:type owl:Class ;
          rdfs:subClassOf :MapType ,
                          :ModifierOverrideType ,
                          :ParameterType ,
                          :SimpleType ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasOption ;
                            owl:someValuesFrom xsd:string
                          ] ;
          rdfs:label "Enum Type" .


###  http://www.solidityontology.oeg.upm.es#EventSpecification
:EventSpecification rdf:type owl:Class ;
                    rdfs:subClassOf [ rdf:type owl:Restriction ;
                                      owl:onProperty :hasEventArguments ;
                                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass :ParameterVariableSpecification
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :hasName ;
                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onDataRange xsd:string
                                    ] ;
                    rdfs:label "Event Specification" .


###  http://www.solidityontology.oeg.upm.es#FRBehaviour
:FRBehaviour rdf:type owl:Class ;
             rdfs:comment "Fallback / Receive behaviour" ;
             rdfs:label "Fallback Receive Behaviour" .


###  http://www.solidityontology.oeg.upm.es#FRVisibility
:FRVisibility rdf:type owl:Class ;
              rdfs:label "Fallback Receive Visibility" .


###  http://www.solidityontology.oeg.upm.es#FallbackSpecification
:FallbackSpecification rdf:type owl:Class ;
                       rdfs:subClassOf [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasReceiveVisibility ;
                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass :FRVisibility
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasReceiveBehaviour ;
                                         owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass :FRBehaviour
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasCode ;
                                         owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onDataRange xsd:string
                                       ] ;
                       rdfs:comment "It is called when a non-existent function is called on the contract. This function always receives data, but to also receive Ethereum, function must be payable." ;
                       rdfs:label "Fallback Specification" .


###  http://www.solidityontology.oeg.upm.es#FixedMxN
:FixedMxN rdf:type owl:Class ;
          rdfs:subClassOf :DualMemoryType ;
          rdfs:label "FixedMxN" .


###  http://www.solidityontology.oeg.upm.es#FixedValue
:FixedValue rdf:type owl:Class ;
            rdfs:subClassOf :SimpleValue ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasValue ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:float
                            ] ;
            rdfs:label "Fixed Value" .


###  http://www.solidityontology.oeg.upm.es#FunctionBehaviour
:FunctionBehaviour rdf:type owl:Class ;
                   rdfs:subClassOf [ rdf:type owl:Restriction ;
                                     owl:onProperty :includes ;
                                     owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass :ModifierSpecification
                                   ] ;
                   rdfs:label "Function Behaviour" .


###  http://www.solidityontology.oeg.upm.es#GenericValue
:GenericValue rdf:type owl:Class ;
              rdfs:subClassOf :SimpleValue ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasValue ;
                                owl:cardinality "1"^^xsd:nonNegativeInteger
                              ] ;
              rdfs:label "Generic Value" .


###  http://www.solidityontology.oeg.upm.es#Implementation
:Implementation rdf:type owl:Class ;
                rdfs:subClassOf [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasImports ;
                                  owl:someValuesFrom xsd:string
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasName ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onDataRange xsd:string
                                ] ;
                rdfs:comment "There are two types of smart contract implementations, contracts and libraries" ;
                rdfs:label "Implementation" .


###  http://www.solidityontology.oeg.upm.es#Int
:Int rdf:type owl:Class ;
     rdfs:subClassOf :SingleMemoryType .


###  http://www.solidityontology.oeg.upm.es#IntValue
:IntValue rdf:type owl:Class ;
          rdfs:subClassOf :SimpleValue ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasValue ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:integer
